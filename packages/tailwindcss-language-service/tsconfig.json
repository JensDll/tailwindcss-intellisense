{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "rootDir": "./src",
    "outDir": "dist",
    "module": "ESNext",
    "target": "ES2019",
    "lib": ["ESNext"],
    "declaration": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "downlevelIteration": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "jsx": "react"
  },
  "include": ["src"]
}
